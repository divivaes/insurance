<%- include inc_client/header.ejs %>
<%- include inc_client/menu.ejs %>

            <main class="main-content">
                <div class="breadcrumbs">
                    <div class="container">
                        <a href="/">Главная страница</a>
                        <span><i class="fa fa-arrow-right"></i></span>
                        <a href="/products">Калькуляторы</a>
                        <span><i class="fa fa-arrow-right"></i></span>
                        <span>Недвижимость</span>

                    </div>
                </div>

                <div class="page">
                    <div class="container">
                        <h3 class="entry-title">Недвижимость</h3>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="container">
                                    <div class="row form-group">
                                        <div class="col-md-12">
                                            <ul class="nav nav-pills nav-justified thumbnail setup-panel" id="myNav">
                                                <li id="navStep1" class="li-nav active" step="#step-1">
                                                    <a>
                                                        <h4 class="list-group-item-heading">Шаг 1 </h4>
                                                        <p class="list-group-item-text">Расчет полиса</p>
                                                    </a>
                                                </li>
                                                <li id="navStep2" class="li-nav disabled" step="#step-2">
                                                    <a>
                                                        <h4 class="list-group-item-heading">Шаг 2</h4>
                                                        <p class="list-group-item-text">Анкетные данные</p>
                                                    </a>
                                                </li>
                                                <li id="navStep3" class="li-nav disabled" step="#step-3">
                                                    <a>
                                                        <h4 class="list-group-item-heading">Шаг 3</h4>
                                                        <p class="list-group-item-text">Доставка и оплата</p>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>

                                <form class="container contact-form" action="/insurance/goods/add" method="POST" >
                                    <div class="row setup-content" id="step-1">
                                        <div class="col-md-12">
                                            <div class="col-md-12 well text-center">
                                                <h3> Сведения о имуществе</h3>
                                                <div class="col-md-12">
                                                    <div class="col-md-4">
                                                        <label for="sc1">Объект страхования</label><br>
                                                        <select class="form-group sc1" name="sc1" rel="1">
                                                          <option value="0" selected="selected">Квартира</option>
                                                          <option value="1" >Частный дом</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-md-4">
                                                      <label class="container"> Огонь (пожар, авария, электросети)
                                                        <input type="checkbox" name="r1" id="r1" class="r1 ri" value="Огонь (пожар, авария, электросети)">
                                                        <span class="checkmark"></span>
                                                      </label>
                                                      <label class="container">Залив
                                                        <input type="checkbox" name="r2" id="r2" class="r2 ri" value="Залив">
                                                        <span class="checkmark"></span>
                                                      </label>
                                                      <label class="container">Повреждение
                                                        <input type="checkbox" name="r3" id="r3" class="r3 ri" value="Повреждение">
                                                        <span class="checkmark"></span>
                                                      </label>
                                                      <label class="container">Стихийные бедствия
                                                        <input type="checkbox" name="r4" id="r4" class="r4 ri" value="Стихийные бедствия">
                                                        <span class="checkmark"></span>
                                                      </label>
                                                      <label class="container">Противоправные действия третьих лиц
                                                        <input type="checkbox" name="r5" id="r5" class="r5 ri" value="Противоправные действия третьих лиц">
                                                        <span class="checkmark"></span>
                                                      </label>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="sc3">Страховая сумма</label><br>
                                                        <input  name="sc3" class="sc3 form-group" rel="1" type="text">
                                                        <p>от 3 000 000 до 20 000 000 тг.</p>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                  <div class="form-group col-md-3" style="margin-left: 40px;margin-top: 10px;">
                                                      <input onclick="step1Next()" type="button" class="button" class="btn btn-md btn-info" value="Далее">
                                                  </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row setup-content" id="step-2">
                                        <div class="col-md-12">
                                            <div class="col-md-12 well text-center">
                                                <h1 class="text-center"> Анкетные данные</h1>
                                                <div class="container col-md-12">
                                                    <div class="col-md-4">
                                                        <label for="client_city">Город</label><br>
                                                        <select class="form-group" name="client_city" id="client_city" aria-required="true">
                                                            <option value="">Укажите Ваш город</option>
                                                            <option value="Актау">Актау</option>
                                                            <option value="Актобе">Актобе</option>
                                                            <option value="Алматы">Алматы</option>
                                                            <option value="Астана">Астана</option>
                                                            <option value="Атырау">Атырау</option>
                                                            <option value="Жезказган">Жезказган</option>
                                                            <option value="Караганда">Караганда</option>
                                                            <option value="Кокшетау">Кокшетау</option>
                                                            <option value="Костанай">Костанай</option>
                                                            <option value="Кызылорда">Кызылорда</option>
                                                            <option value="Павлодар">Павлодар</option>
                                                            <option value="Петропавловск">Петропавловск</option>
                                                            <option value="Семей">Семей</option>
                                                            <option value="Талдыкорган">Талдыкорган</option>
                                                            <option value="Тараз">Тараз</option>
                                                            <option value="Уральск">Уральск</option>
                                                            <option value="Усть-Каменогорск">Усть-Каменогорск</option>
                                                            <option value="Шымкент">Шымкент</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="client_fullname">ФИО</label><br>
                                                        <input type="text" name="client_fullname" class="form-group" placeholder="Ваше ФИО">
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="client_phone">Телефон</label><br>
                                                        <input type="text" name="client_phone" class="form-group phone" placeholder="Ваш телефон">
                                                    </div>
                                                    <div class="col-md-12 text-center">
                                                        <div class="row">
                                                            <div class="col-md-4">
                                                                <label for="client_email">Электронная почта</label><br>
                                                                <input type="text" name="client_email" class="form-group" placeholder="Ваша электронная почта">
                                                            </div>
                                                            <div class="col-md-7">
                                                                <label for="client_address">Адрес</label><br>
                                                                <textarea name="client_address"  placeholder="Полный адрес проживания и дополнительные комментарии"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-12 text-center">
                                                        <div class="row" style="margin-left: 30px;">
                                                            <div class="col-md-3">
                                                              <input type="button" onclick="prevStep()" class="button" value="Назад">
                                                            </div>
                                                            <div class="col-md-3 ">
                                                                <input type="button" onclick="step2Next()" class="button" value="Далее">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row setup-content" id="step-3">
                                        <div class="col-xs-12">
                                            <div class="col-md-12 well text-center">
                                                <h1 class="text-center"> Доставка и оплата</h1>
                                                <div class="col-md-12 text-center">
                                                    <div class="row">
                                                        <div class="col-md-6" style="margin-top: -15px;">
                                                            <table width="100%" style="margin-top: 0px;">
                                                                <tr>
                                                                    <td width="100%" align="left" colspan="3" valign="top" style="padding-left:0px !important;">
                                                                        <label for="payment_type">Выберите способ оплаты</label><br>
                                                                        <select class="form-group" name="payment_type" id="payment_type" aria-required="true">
                                                                            <option value="">Выберите из списка</option>
                                                                            <option value="Наличными при получении курьеру">Наличными при получении курьеру</option>
                                                                            <option value="Оформить в офисе">Оформить в офисе</option>
                                                                        </select>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </div>
                                                        <div class="col-md-2 form-group">
                                                            <input type="button" onclick="prevStep()" class="button" value="Назад">
                                                        </div>
                                                        <div class="col-md-2 form-group">
                                                            <a href="javascript:calcGoods()" class="button">Узнать стоимость</a>
                                                            <div class="result"> Стоимость полиса - 0 тг. </div>
                                                            <div class="send"></div>
                                                        </div>
                                                        <div class="col-md-2 form-group append"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </main>


<%- include inc_client/footer.ejs %>
